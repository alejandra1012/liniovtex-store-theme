{
    
  "flex-layout.row#comentarios__about-us": {
    "title": "container de los comentarios sobre lINIO",
    "children": ["flex-layout.col#comentarios__about-us"],
    "props": {
      "rowGap":7,
      "blockClass": "container__comentarios--about-us"
    }
  },
  "flex-layout.col#comentarios__about-us": {
    "title": "comentarios sobre linio",
    "children": [
      "rich-text#titulo__comentarios--about-us",
      "flex-layout.row#img__comentarios-sobre-linio"
    ],
    "props":{
      "blockClass": "title-comentarios__about-us"
    }
  },
  "rich-text#titulo__comentarios--about-us": {
    "title":"titulo comentarios sobre linio",
