{
    "header": {
        "title": "Header",
        "blocks": [
            "header-layout.desktop",
            "header-layout.mobile"
        ]
    },
    "header.full": {
        "title": "Header",
        "blocks": [
            "header-layout.desktop",
            "header-layout.mobile"
        ]
    },
    "header-layout.desktop": {
        "title": "Header Desktop",
        "children": [
            "header-row#header__top",
            "header-row#header__middle"
        ]
    },
    "header-layout.mobile": {
        "title": "Header Mobile",
        "children": [
            "header-row#header__top",
            "header-row#header__middle"
        ]
    },
    "header-row#header__top": {
        "title": "Header Superior",
        "children": [
            "logo#header__top--logo",
            "header-spacer",
            "login",
            "minicart.v2"
        ],
        "props": {
            "blockClass": "header__top"
        }
    },
    "logo#header__top--logo": {
        "title": "Logo",
        "props": {
            "url": "assets/img/header__logo-desktop.png",
            "width": 120,
            "href": "/"
        }
    },
    "login": {
        "props": {
            "emailAndPasswordTitle": "LOG-IN",
            "accessCodeTitle": "Acess Code LOG-IN",
            "emailPlaceholder": "e-mail",
            "passwordPlaceholder": "password",
            "showPasswordVerificationIntoTooltip": true,
            "showIconProfile": true,
            "text": "Iniciar Sesi√≥n"
        }
    },
    "header-row#header__middle": {
        "title": "Header medio",
        "children": [
            "drawer#header__mobile--menu"
        ],
        "props": {
            "blockClass": "header__middle"
        }
    },
    "drawer#header__mobile--menu": {
        "children": []
    }
}
