{
    "responsive-layout.phone#phone__department":{
        "title": "Vista phone busqueda por departamento",
        "children":[
            "flex-layout.row#department__results"
        ],
        "props":{
            "blockClass":"searh-result__phone--department"
        }
      },
      "flex-layout.row#department__results":{
        "title":"Filtro y contenido busqueda por departamento",
        "children":[
            "flex-layout.row#department__results--filter",
            "flex-layout.col#department__results--content"
        ]
      },
      "flex-layout.row#department__results--filter":{
        "children":[
            "gallery-layout-switcher",
            "order-by.v2",
            "filter-navigator.v3"
        ],
        "props":{
            "width": "75%",
            "preserveLayoutOnMobile": true
        }
      },
      "flex-layout.col#department__results--content":{
        "children":[
            "search-content#mobile__content--products-department"
        ],
        "props":{
            "width":"75%"
        }
      },
      "search-content#mobile__content--products-department":{
        "blocks":[
            "gallery#global",
            "not-found"
        ]
    }
}